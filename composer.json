{
    "name": "yuriizee/sensebank-installment-sdk",
    "description": "sensebank-installment-sdk",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yuriizee\\SenseBankInstallmentSDK\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Yurii Zub",
            "email": "yuriyzee@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
