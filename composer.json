        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "7.9.x-dev"
    },
    "require-dev": {
        "symfony/var-dumper": "7.1.x-dev",
        "nunomaduro/phpinsights": "dev-master",
        "phpunit/phpunit": "10.5.x-dev"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
